@{
    ViewData["Title"] = "Lista de Productos"; // Cambia el título según sea necesario
}

@using MVC.Models
@model List<Producto>

<h1>Lista de Productos</h1>

<table class="table table-striped table-dark">
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Descripcion</th>
      <th scope="col">Precio</th>
    </tr>
  </thead>
  <tbody>
    @foreach(var p in Model)
    {
    <tr>
      <td>@p.IdProducto</td>
      <td>@p.Descripcion</td>
      <td>@p.Precio</td>
    </tr>
    }
  </tbody>
</table>

